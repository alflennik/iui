{
  "name": "iui",
  "displayName": "Official Extension for the IUI Programming Language",
  "publisher": "iui",
  "repository": "https://github.com/alflennik/iui",
  "description": "Syntax highlighting for the iui programming language as well as file icons and code folding support.",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.99.3"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "iui",
      "aliases": [],
      "extensions": ["iui"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./iui-light.png",
        "dark": "./iui-dark.png"
      }
    }],
    "grammars": [{
      "language": "iui",
      "scopeName": "source.iui",
      "path": "./syntaxes/iui.tmLanguage.json"
    }]
  }
}
